    "from turtle import Turtle, Screen\n",
    "\n",
    "k = Turtle()\n",
    "k.speed(0)\n",
    "k.color(\"#FF8C00\")\n",
    "\n",
    "s = Screen()\n",
    "s.bgcolor('black')\n",
    "\n",
    "k.penup()\n",
    "k.begin_fill()\n",
    "\n",
    "s.exitonclick()  # Use this to keep the window open until you click it\n"
